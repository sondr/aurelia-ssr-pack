{
  "name": "aurelia-ssr-pack",
  "version": "0.0.1",
  "description": "Aurelia 1 SSR Package",
  "repository": {
    "type": "git",
    "url": "https://github.com/sondr/aurelia-ssr-pack"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build:ts": "tsc",
    "build:other": "cpx ./src/**/*.{html,css} ./dist",
    "build": "npm run build:ts && npm run build:other"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "aurelia",
    "au",
    "ssr"
  ],
  "author": "Sondre Tveit Ernoe",
  "license": "MIT",
  "main": "dist/index.js",
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "cpx2": "^4.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "aurelia-framework": "^1.0.0",
    "aurelia-pal": "^1.0.0",
    "aurelia-loader-webpack": "^2.0.0",
    "jsdom": "^20.0.0",
    "koa": "^2.0.0",
    "preboot": "^8.0.0"
  }
}